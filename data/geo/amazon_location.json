      "url": "https://docs.aws.amazon.com/IAM/latest/UserGuide/reference_policies_condition-keys.html#condition-keys-tagkeys"
    },
    "geo:DeviceIds": {
      "description": "Filters access by the presence of device ids in the request",
      "type": "ArrayOfString",
      "url": "https://docs.aws.amazon.com/location/latest/developerguide/security_iam_service-with-iam.html#security_iam_service-with-iam-id-based-policies-conditionkeys"
    },
    "geo:GeofenceIds": {
      "description": "Filters access by the presence of geofence ids in the request",
      "type": "ArrayOfString",
      "url": "https://docs.aws.amazon.com/location/latest/developerguide/security_iam_service-with-iam.html#security_iam_service-with-iam-id-based-policies-conditionkeys"
